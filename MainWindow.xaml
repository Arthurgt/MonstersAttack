        <Canvas Grid.Column="2" HorizontalAlignment="Left" Height="100" Margin="176,68,0,0" Grid.Row="1" VerticalAlignment="Top" Width="100" RenderTransformOrigin="0.467,0.507"/>
    </Grid>
</Window>
