        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="26.785"/>
            <ColumnDefinition Width="133.215"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="160"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="81"/>
            <RowDefinition/>
            <RowDefinition Height="80"/>
        </Grid.RowDefinitions>
        <Button Content="Start!" HorizontalAlignment="Left" Margin="19,13,0,0" Grid.Row="2" VerticalAlignment="Top" Width="75" Height="20" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2"/>
        <StackPanel Grid.Column="3" Margin="29,17,49,18" Orientation="Vertical" Grid.Row="2">
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="16" Width="65"><Run Text="Ich unika"/><Run Text="j!"/></TextBlock>
